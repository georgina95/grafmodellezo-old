{
	"_version": "1.12.0",
	"sap.app": {
		"id": "elte.Grafmodellezo",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"twitter.xsodata": {
				"uri": "/public/grafmodellezo/twitter/service/twitter.xsodata/",
				"type": "OData",
				"settings": {
					"localUri": "localService/twitter.xsodata/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "elte.Grafmodellezo.view.Main",
			"type": "XML",
			"async": true,
			"id": "Main"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "elte.Grafmodellezo.i18n.i18n"
				}
			},
			"Twitter": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "twitter.xsodata",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "elte.Grafmodellezo.view",
				"controlAggregation": "",
				"controlId": "layoutMain",
				"clearControlAggregation": true,
				"bypassed": {
					"target": []
				}
			},
			"routes": [{
				"name": "Home",
				"pattern": "",
				"target": ["Home"]
			}, {
				"name": "ManageData",
				"pattern": "ManageData",
				"target": ["ManageData"]
			}, {
				"name": "Graph1",
				"pattern": "Graph1",
				"target": ["Graph1"]
			}, {
				"name": "Graph2",
				"pattern": "Graph2",
				"target": ["Graph2"]
			}, {
				"name": "Graph3",
				"pattern": "Graph3",
				"target": ["Graph3"]
			}, {
				"name": "ProjectDetails",
				"pattern": "ProjectDetails",
				"target": ["ProjectDetails"]
			}, {
				"name": "References",
				"pattern": "References",
				"target": ["References"]
			}, {
				"name": "DetailTweets",
				"pattern": "tweets/{ID}",
				"titleTarget": "",
				"greedy": false,
				"target": ["ManageData", "DetailTweets"]
			}, {
				"name": "DetailUsers",
				"pattern": "users/{ID}",
				"titleTarget": "",
				"greedy": false,
				"target": ["ManageData", "DetailUsers"]
			}, {
				"name": "DetailPlaces",
				"pattern": "places/{ID}",
				"titleTarget": "",
				"greedy": false,
				"target": ["ManageData", "DetailPlaces"]
			}],
			"targets": {
				"Home": {
					"viewType": "XML",
					"viewName": "Home",
					"controlAggregation": "beginColumnPages"
				},
				"ManageData": {
					"viewType": "XML",
					"viewName": "ManageData",
					"controlAggregation": "beginColumnPages",
					"viewPath": "elte.Grafmodellezo.view.dataManagement"
				},
				"Graph1": {
					"viewType": "XML",
					"viewName": "Graph1",
					"controlAggregation": "beginColumnPages",
					"viewPath": "elte.Grafmodellezo.view.graphs"
				},
				"Graph2": {
					"viewType": "XML",
					"viewName": "Graph2",
					"controlAggregation": "beginColumnPages",
					"viewPath": "elte.Grafmodellezo.view.graphs"
				},
				"Graph3": {
					"viewType": "XML",
					"viewName": "Graph3",
					"controlAggregation": "beginColumnPages",
					"viewPath": "elte.Grafmodellezo.view.graphs"
				},
				"ProjectDetails": {
					"viewType": "XML",
					"viewName": "ProjectDetails",
					"controlAggregation": "beginColumnPages",
					"viewPath": "elte.Grafmodellezo.view.technical"
				},
				"References": {
					"viewType": "XML",
					"viewName": "References",
					"controlAggregation": "beginColumnPages",
					"viewPath": "elte.Grafmodellezo.view.technical"
				},
				"DetailHashtags": {
					"viewType": "XML",
					"viewName": "DetailHashtags",
					"viewPath": "elte.Grafmodellezo.view.dataManagement",
					"controlAggregation": "midColumnPages"
				},
				"DetailPlaces": {
					"viewType": "XML",
					"viewName": "DetailPlaces",
					"viewPath": "elte.Grafmodellezo.view.dataManagement",
					"controlAggregation": "midColumnPages"
				},
				"DetailTweets": {
					"viewType": "XML",
					"viewName": "DetailTweets",
					"viewPath": "elte.Grafmodellezo.view.dataManagement",
					"controlAggregation": "midColumnPages"
				},
				"DetailUsers": {
					"viewType": "XML",
					"viewName": "DetailUsers",
					"viewPath": "elte.Grafmodellezo.view.dataManagement",
					"controlAggregation": "midColumnPages"
				},
				"DetailUsermentions": {
					"viewType": "XML",
					"viewName": "DetailUsermentions",
					"viewPath": "elte.Grafmodellezo.view.dataManagement",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}