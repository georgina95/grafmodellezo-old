<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.f." xmlns="sap.m" class="sapUiSizeCompact" displayBlock="true"
	controllerName="elte.Grafmodellezo.controller.Main">
	<Shell id="shell" appWidthLimited="false" title="{i18n>appTitle}">
		<Page title="{i18n>appTitle}" showHeader="true" showFooter="false" showSubHeader="false" 
			showNavButton="true" navButtonPress="onNavBack">
